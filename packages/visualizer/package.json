{
	"name": "l-log-vis",
	"version": "0.2.6",
	"description": "A beautiful visualizer for LLM interaction logs",
	"scripts": {
		"d": "vite",
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"type": "tsc --noEmit",
		"server": "bun run server.ts",
		"dev:full": "bun run server.ts & bun run dev",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/loclv/llm-lean-log.git",
		"directory": "packages/visualizer"
	},
	"bugs": {
		"url": "https://github.com/loclv/llm-lean-log/issues"
	},
	"homepage": "https://github.com/loclv/llm-lean-log/tree/main/packages/visualizer#readme",
	"type": "module",
	"bin": {
		"l-log-vis": "./src/cli.ts"
	},
	"files": [
		"dist",
		"src/cli.ts"
	],
	"dependencies": {
		"framer-motion": "^12.29.0",
		"lucide-react": "^0.562.0",
		"llm-lean-log-core": "workspace:*",
		"papaparse": "^5.5.3",
		"prism-react-renderer": "^2.4.1",
		"react": "^19.2.3",
		"react-dom": "^19.2.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@types/node": "^25.0.10",
		"@types/papaparse": "^5.5.2",
		"@types/react": "^19.2.9",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"bun-types": "^1.3.6",
		"eslint": "^9.39.2",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.26",
		"globals": "^16.5.0",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^7.3.1"
	},
	"engines": {
		"bun": ">=1.3.6"
	},
	"keywords": [
		"llm",
		"logging",
		"visualizer",
		"csv",
		"token-optimization"
	],
	"license": "MIT"
}
